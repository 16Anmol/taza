{
  "expo": {
    "name": "MandiKharidari",
    "slug": "mandikharidari",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/madikharidari.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/madikharidari.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#D97706"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/madikharidari.jpg",
        "backgroundColor": "#D97706"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION", 
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_SMS",
        "READ_SMS"
      ]
    },
    "web": {
      "favicon": "./assets/madikharidari.jpg"
    },
    "plugins": [
      "expo-location",
      "expo-sms"
    ],
    "sdkVersion": "53.0.0"
  }
}